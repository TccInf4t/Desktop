package br.com.onpecas.model;

public class Estado {
	private int oid_estado;
	private int nome;

	public int getOid_estado() {
		return oid_estado;
	}
	public void setOid_estado(int oid_estado) {
		this.oid_estado = oid_estado;
	}
	public int getNome() {
		return nome;
	}
	public void setNome(int nome) {
		this.nome = nome;
	}


}
